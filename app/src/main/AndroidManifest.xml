<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lazytomatostudios.interrupt18">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.lazytomatostudios.interrupt18.AppController"
        android:allowBackup="true"
        android:icon="@drawable/applogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.NoActionBar">
        <activity android:name="com.lazytomatostudios.interrupt18.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.lazytomatostudios.interrupt18.EventActivity" />
        <activity android:name="com.lazytomatostudios.interrupt18.TransportActivity" />
        <activity android:name="com.lazytomatostudios.interrupt18.InstructionActivity" />
        <activity android:name="com.lazytomatostudios.interrupt18.RegisterActivity" />
        <activity android:name="com.lazytomatostudios.interrupt18.Registration_Events" />
        <activity android:name="com.lazytomatostudios.interrupt18.MainActivity"></activity>
    </application>

</manifest>