<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp"
    android:background="#212121"
    android:gravity="center"
    android:id="@+id/ll"
    tools:context="com.example.arvind.svceinterrupt.Registration_Events">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"

        android:background="#212121"
        android:gravity="center">

        <TextView
            android:fontFamily="@font/monts"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="REGISTER"
            android:gravity="center"
            android:textStyle="bold"/>


        <LinearLayout
            android:clipToPadding="false"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="10dp"
            android:layout_gravity="center"
            android:orientation="horizontal"
            android:background="#212121"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <android.support.v7.widget.CardView
                android:id="@+id/flexe"
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                android:layout_width="90dp"
                app:cardCornerRadius="10dp"
                android:layout_height="90dp"
                app:cardBackgroundColor="#000000"
                app:cardPreventCornerOverlap="true"
                android:layout_margin="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ripple"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/css"
                        android:padding="10dp"/>
                    <TextView
                        android:fontFamily="@font/monts"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:textSize="10dp"
                        android:layout_marginTop="5dp"
                        android:text="Flex Box Froggy"
                        android:textColor="#ffffff"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                android:id="@+id/ichallengee"
                android:layout_width="90dp"
                app:cardCornerRadius="10dp"
                android:layout_marginLeft="15dp"
                android:layout_height="90dp"
                app:cardBackgroundColor="#000000"
                app:cardPreventCornerOverlap="true"
                android:layout_margin="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ripple"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"

                        android:src="@drawable/b4"
                        android:padding="10dp"/>

                    <TextView
                        android:gravity="center"
                        android:fontFamily="@font/monts"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:layout_marginTop="10dp"
                        android:textSize="10dp"
                        android:text="Interrupt challenge"
                        android:textColor="#ffffff"
                        android:id="@+id/textView7" />

                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                android:id="@+id/generice"
                android:layout_width="90dp"
                app:cardCornerRadius="10dp"
                android:layout_height="90dp"
                app:cardBackgroundColor="#000000"
                app:cardPreventCornerOverlap="true"
                android:layout_margin="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ripple"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"

                        android:src="@drawable/code"
                        android:padding="10dp"/>
                    <TextView
                        android:fontFamily="@font/monts"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:textSize="10dp"
                        android:layout_marginTop="10dp"
                        android:text="Generic Coding"
                        android:textColor="#ffffff"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>




        </LinearLayout>

        <LinearLayout
            android:clipToPadding="false"
            android:layout_marginLeft="5dp"
            android:orientation="horizontal"
            android:background="#212121"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <android.support.v7.widget.CardView
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                android:layout_width="90dp"
                app:cardCornerRadius="10dp"
                android:layout_height="90dp"
                android:id="@+id/terminale"
                app:cardBackgroundColor="#000000"
                app:cardPreventCornerOverlap="true"
                android:layout_margin="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ripple"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/terminal"
                        android:padding="10dp"/>
                    <TextView
                        android:gravity="center"
                        android:fontFamily="@font/monts"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:layout_marginTop="10dp"
                        android:textSize="10dp"
                        android:text="Terminal Of Secrets"
                        android:textColor="#ffffff"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                android:id="@+id/logice"
                android:layout_width="90dp"
                app:cardCornerRadius="10dp"
                android:layout_height="90dp"
                app:cardBackgroundColor="#000000"
                app:cardPreventCornerOverlap="true"
                android:layout_margin="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ripple"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"

                        android:src="@drawable/logical"
                        android:padding="10dp"/>
                    <TextView
                        android:fontFamily="@font/monts"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:textSize="10dp"
                        android:layout_marginTop="10dp"
                        android:text="Logician's Code"
                     android:textColor="#ffffff"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                android:id="@+id/pitche"
                android:layout_width="90dp"
                app:cardCornerRadius="10dp"
                android:layout_height="90dp"
                app:cardBackgroundColor="#000000"
                app:cardPreventCornerOverlap="true"
                android:layout_margin="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ripple"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/marketing"
                        android:padding="10dp"/>
                    <TextView
                        android:fontFamily="@font/monts"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:textSize="10dp"
                        android:layout_marginTop="10dp"
                        android:text="Pitch Perfect"
                        android:textColor="#ffffff"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>



        </LinearLayout>


        <LinearLayout
            android:clipToPadding="false"
            android:layout_marginLeft="5dp"
            android:orientation="horizontal"
            android:background="#212121"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <android.support.v7.widget.CardView
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                android:layout_width="90dp"
                android:id="@+id/inquize"
                app:cardCornerRadius="10dp"
                android:layout_height="90dp"
                app:cardBackgroundColor="#000000"
                app:cardPreventCornerOverlap="true"
                android:layout_margin="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ripple"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"

                        android:src="@drawable/quiz"
                        android:padding="10dp"/>
                    <TextView
                        android:fontFamily="@font/monts"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:textSize="10dp"
                        android:layout_marginTop="10dp"
                        android:text="Inquiztive"
                        android:textColor="#ffffff"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                android:layout_width="90dp"
                android:id="@+id/pipee"
                app:cardCornerRadius="10dp"
                android:layout_height="90dp"
                app:cardBackgroundColor="#000000"
                app:cardPreventCornerOverlap="true"
                android:layout_margin="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ripple"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/code2"
                        android:padding="10dp"/>
                    <TextView
                        android:fontFamily="@font/monts"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:layout_marginTop="10dp"
                        android:textSize="10dp"
                        android:text="Pipe The Piper"
                        android:textColor="#ffffff"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                android:layout_width="90dp"
                app:cardCornerRadius="10dp"
                android:id="@+id/dse"
                android:layout_height="90dp"
                app:cardBackgroundColor="#000000"
                app:cardPreventCornerOverlap="true"
                android:layout_margin="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ripple"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"

                        android:src="@drawable/analytics"
                        android:padding="10dp"/>
                    <TextView
                        android:fontFamily="@font/monts"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:textSize="10dp"
                        android:layout_marginTop="10dp"
                        android:text="Data Science"
                        android:textColor="#ffffff"/>

                </LinearLayout>
            </android.support.v7.widget.CardView>

        </LinearLayout>

        <LinearLayout
            android:clipToPadding="false"
            android:orientation="horizontal"
            android:layout_marginLeft="5dp"
            android:background="#212121"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <android.support.v7.widget.CardView

                android:clickable="true"
                android:focusable="true"
                android:layout_width="90dp"
                android:id="@+id/technoe"
                app:cardCornerRadius="10dp"
                android:layout_height="90dp"
                app:cardBackgroundColor="#000000"
                app:cardPreventCornerOverlap="true"
                android:layout_margin="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:background="@drawable/ripple"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/techcrunch"
                        android:padding="10dp"/>
                    <TextView
                        android:fontFamily="@font/monts"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:textSize="10dp"
                        android:layout_marginTop="10dp"
                        android:text="Techno Fair"
                        android:textColor="#ffffff"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                android:layout_width="90dp"
                app:cardCornerRadius="10dp"
                android:id="@+id/ppte"
                android:layout_height="90dp"
                app:cardBackgroundColor="#000000"
                app:cardPreventCornerOverlap="true"
                android:layout_margin="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ripple"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"

                        android:src="@drawable/present"
                        android:padding="10dp"/>
                    <TextView

                        android:fontFamily="@font/monts"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:textSize="10dp"
                        android:layout_marginTop="10dp"
                        android:text="Presentation Hub"
                        android:textColor="#ffffff"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                android:id="@+id/awse"
                android:layout_width="90dp"
                app:cardCornerRadius="10dp"
                android:layout_height="90dp"
                app:cardBackgroundColor="#000000"
                app:cardPreventCornerOverlap="true"
                android:layout_margin="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ripple"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/workshop"
                        android:padding="10dp"/>
                    <TextView
                        android:fontFamily="@font/monts"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:layout_marginTop="10dp"
                        android:text="Workshop"
                        android:textSize="10dp"
                        android:textColor="#ffffff"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>




        </LinearLayout>




        <Button
            android:fontFamily="@font/monts"
            android:id="@+id/register"
            android:textColorHint="#ffffff"
            android:layout_gravity="center"
            android:hint="Register"
            android:textColor="#FFFFFF"
            android:textSize="20dp"
            android:layout_marginTop="35dp"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:background="@drawable/buttonshape"
            android:shadowColor="#A8734A"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="5"
            />


    </LinearLayout>

</ScrollView>


